{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
     "resolveJsonModule": true,
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*", "src/*"]
    }
  },
  "include": ["src"]
}